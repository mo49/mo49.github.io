{
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "prepare": "husky install",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
