    "@typescript-eslint/parser": "^5.18.0",
    "@vue/test-utils": "^2.0.0-rc.19",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "normalize.css": "^8.0.1",
    "nuxt3": "latest",
