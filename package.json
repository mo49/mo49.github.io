  "lint-staged": {
    "*.{js,ts,vue}": [
      "yarn lint:js --fix"
    ]
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vue/test-utils": "^2.0.0-rc.19",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nuxt3": "latest",
    "typescript": "^4.6.3"
  }
}
